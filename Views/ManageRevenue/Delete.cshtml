@model LinkshellManager.Models.Income

@{
    ViewData["Title"] = "Delete Income";
}

<h2>Delete Income</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Income</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Linkshell Name
        </dt>
        <dd class="col-sm-10">
            @Model.LinkshellName
        </dd>
        <dt class="col-sm-2">
            Method Of Income
        </dt>
        <dd class="col-sm-10">
            @Model.MethodOfIncome
        </dd>
        <dt class="col-sm-2">
            Details
        </dt>
        <dd class="col-sm-10">
            @Model.Details
        </dd>
        <dt class="col-sm-2">
            Value
        </dt>
        <dd class="col-sm-10">
            @Model.Value
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>